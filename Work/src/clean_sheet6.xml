                    <xsd:documentation>
                        Payment method of the subscriber
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="customer" type="cdm:Customer" minOccurs="0"/>
            <xsd:element name="parent" type="cdm:Customer" minOccurs="0"/>
            <xsd:element name="paymentResponsible" type="cdm:Customer" minOccurs="0"/>
            <xsd:element name="billCycle" type="tns:BillingCycle" minOccurs="0"/>
            <xsd:element name="directDebit" type="tns:DirectDebit" minOccurs="0"/>
        </xsd:sequence>
