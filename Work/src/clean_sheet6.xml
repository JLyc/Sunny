    <xsd:complexType name="MobilePostpaidCustomer" >
        <xsd:sequence>
            <xsd:annotation>
                <xsd:documentation>
                    Mobile postpaid specific attributes
                </xsd:documentation>
            </xsd:annotation>
            <xsd:element name="level" type="xsd:string" minOccurs="0" nillable="false">
                <xsd:annotation>
                    <xsd:documentation>
                        Customer level, e.g. business partner, subscriber, dealer, etc.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="priceGroupId" type="xsd:string" minOccurs="0" nillable="false">
                <xsd:annotation>
                    <xsd:documentation>
                        Price group code
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="costCentreId" type="xsd:string" minOccurs="0" nillable="false">
                <xsd:annotation>
                    <xsd:documentation>
                        Cost center code
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="paymentType" type="xsd:string" minOccurs="0" nillable="false">
