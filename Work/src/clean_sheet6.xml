    <xsd:complexType name="GetContactStageRequest" >
        <xsd:sequence>
            <xsd:element name="maxRecords" type="xsd:string" minOccurs="0" nillable="false">
                <xsd:annotation>
                    <xsd:documentation>
                        Maximal records of Contacts in response
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice>
                <xsd:element name="customerId" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Unique identifier of Customer over all systems.

                            Customer Id (in CDM format, e.g. ‘SIEBEL-PRD-CUSTOMER-12345’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="customerRowId" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            rowid of Account entity in Siebel CRM

                            Customer rowId (e.g. ‘1-8VKJ’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="customerCode" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Customer Code (e.g. ‘1723239400’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="customerSalesNumber" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            ICO - Company registration number (e.g. ‘35760273’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="contactRowId" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Contact rowId (e.g. ‘1-9XLW’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="contactId" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Contact Id (in CDM format, e.g. ‘SIEBEL-PRD-ADDRESS-12345’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="contactPersonalID" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Contact Person Identifier. (Rodne cislo) (e.g. ‘8212241234’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

