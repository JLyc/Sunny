            <xsd:element name="custCode" type="xsd:string" nillable="false"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetMobilePostpaidCustomerRequest" >
        <xsd:sequence>
            <xsd:choice>
                <xsd:element name="customerId">
                    <xsd:annotation>
                        <xsd:documentation>
                            Integration ID of the BSCS customer, i.e. &#39;BSCS-PRD-CUSTOMER-&#39; concatenated with the numerical value stored in sysadm.CUSTOMER_ALL.CUSTOMER_ID
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>

                        <xsd:restriction base="xsd:string">   <xsd:pattern value="((GSITY)|(IVR)|(GENESYS)|(XTC)|(DMS)|(MNP)|(SAM)|(SELFCARE)|(HPSD)|(FUNDIAL)|(BSCS)|(SICAP)|(INPPS)|(DOBS)|(CLARIFY)|(TOWER)|(LDAP)|(SIEBEL)|(ILINK)|(INFINYS)|(MAGIO)|(RMCA)|(NBE)|(NCC)|(IMD)|(OTA)|(TPM)|(NCBS)|(SAPFI)|(PORTAL)|(PURITY)|(DUMO)|(BPOINT)|(GISG)|(EC)|(MWF))-((DC)|(DEV2)|(DEV)|(TST)|(PRD))-((\p{Lu})+)-((\p{Nd}|\p{L}|\p{P}|\p{Sm})+)"/> </xsd:restriction>

                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="custCode" type="xsd:string" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Unique BSCS customer code, as stored in sysadm.CUSTOMER_ALL.CUSTCODE
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="msisdn" type="xsd:string" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            The main directory number of the customer&#39;s contracted postpaid product
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Rateplan" >
