    <xsd:complexType name="BillingCycle" >
        <xsd:sequence>
            <xsd:element name="code" type="xsd:string" minOccurs="0" nillable="false">
                <xsd:annotation>
                    <xsd:documentation>
                        Billing cycle code
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dayFrom" type="xsd:dateTime" minOccurs="0" nillable="false">
                <xsd:annotation>
                    <xsd:documentation>
                        The first day of the billiling period
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dayTo" type="xsd:dateTime" minOccurs="0" nillable="false">
                <xsd:annotation>
                    <xsd:documentation>
                        The last day of the billing period
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="FetchMarketRequest" >
        <xsd:sequence>
            <xsd:element name="customerId" type="xsd:string" minOccurs="0" nillable="false"/>
            <xsd:element name="contractId" type="xsd:string" minOccurs="0" nillable="false"/>
            <xsd:element name="msisdn" type="xsd:string" minOccurs="0" nillable="false"/>
            <xsd:element name="personalNumber" type="xsd:string" minOccurs="0" nillable="false"/>
            <xsd:element name="companyRegNo" type="xsd:string" minOccurs="0" nillable="false"/>
            <xsd:element name="cacheAllowed" type="xsd:boolean" minOccurs="0" nillable="false">
                <xsd:annotation>
                    <xsd:documentation>
                        True/False operator specifying, whether service call is allowed to use cached result (when available). If not provided or set to TRUE, caching is allowed. If explicitly set to FALSE, result will always be retrieved from source, omitting caching process.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="FetchMarketResponse" >
        <xsd:sequence>
