    <xsd:complexType name="GetCustomerRequest" >
        <xsd:sequence>
            <xsd:choice>
                <xsd:element name="msisdn" type="xsd:string" nillable="false"/>
                <xsd:element name="customerId" type="xsd:string" nillable="false"/>
                <xsd:element name="companyRegNo" type="xsd:string" nillable="false"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetCustomerStageRequest" >
        <xsd:sequence>
            <xsd:element name="extendMode" type="xsd:string" minOccurs="0" nillable="false">
                <xsd:annotation>
                    <xsd:documentation>
                        &quot;Possible values:
                        BP_CN_ADDR = returns Customer including related BillingProfiles, Contacts and Addresses
                        BP_ADDR = returns Customer including related BillingProfiles and Addresses
                        BP_CN = returns Customer including related BillingProfiles and Contacts
                        CN_ADDR = returns Customer including related Contacts and Addresses
                        CN = returns Customer including related Contacts
                        ADDR = returns Customer including related Addresses
                        BP = returns Customer including related BillingProfiles
                        If empty or missing - returns Customer only&quot;

                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="maxRecords" type="xsd:string" minOccurs="0" nillable="false">
                <xsd:annotation>
                    <xsd:documentation>
                        Define limit of returned Customers

                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice>
                <xsd:element name="customerId" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Unique identifier of Customer over all systems.

                            Customer Id (in CDM format, e.g. ‘SIEBEL-PRD-CUSTOMER-12345’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="customerRowId" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            rowid of Account entity in Siebel CRM

                            Customer rowId (e.g. ‘1-8VKJ’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="customerCode" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Customer Code (e.g. ‘1723239400’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="customerSalesNumber" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            ICO - Company registration number (e.g. ‘35760273’)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="customerPersonalID" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Account Person Identifier. (Rodne cislo)

                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="billingProfileNum" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Billing Profile Number (e.g. ‘1234567890)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="assetSerialNum" type="xsd:string" minOccurs="0" nillable="false">
                    <xsd:annotation>
                        <xsd:documentation>
                            Asset Serial Number
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetCustomerResponse" >
        <xsd:sequence>
            <xsd:element name="Customer" type="cdm:Customer"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="UpdateDunningStepMessageIn">
