                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:schema>

        <xsd:schema targetNamespace="http://www.t-mobile.sk/delegation-api/delegationRequests">
            <xsd:element name="delegationRequests">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element type="xsd:long" name="id" minOccurs="0"/>
